<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <GridPane hgap="8.0">
        <columnConstraints>
          <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" />
          <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <CheckBox fx:id="checkbox3" mnemonicParsing="false" text="Onion $0.20" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <RadioButton fx:id="radiobutton1" mnemonicParsing="false" text="Chicken Tikka $2.20" GridPane.rowIndex="2">
               <toggleGroup>
                  <ToggleGroup fx:id="sandwhich" />
               </toggleGroup></RadioButton>
            <RadioButton fx:id="radiobutton2" mnemonicParsing="false" text="Tuna &amp; Mayo $1.20" toggleGroup="$sandwhich" GridPane.rowIndex="3" />
            <RadioButton fx:id="radiobutton3" mnemonicParsing="false" text="Cheese &amp; Onion $1.20" toggleGroup="$sandwhich" GridPane.rowIndex="4" />
            <CheckBox fx:id="checkbox1" mnemonicParsing="false" text="Pickles $0.20" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <CheckBox fx:id="checkbox2" mnemonicParsing="false" text="Lettuce $0.20" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#buttonClicked" text="Calculate" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label text="Sandwhich Bar" textFill="#d31414">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label text="Sandwhiches" textFill="#391fcd" GridPane.rowIndex="1" />
            <Label text="Additional Garnish" textFill="#360cd0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
         <padding>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </padding>
      </GridPane>
   </children>
</GridPane>
